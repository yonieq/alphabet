{
  "name": "vue-toastification",
  "version": "0.8.0",
  "private": false,
  "description": "Toasts for Vue made easy!",
  "author": "Gustavo Maronato",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --mode production",
